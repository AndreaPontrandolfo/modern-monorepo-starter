{
  "name": "@andreapontrandolfo/js-utils",
  "version": "1.0.1",
  "description": "A bunch of Javascript utility functions written in Typescript.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "author": "Andrea Pontrandolfo",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^5.11.13"
  },
  "browser": "dist/index.js",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  },
  "files": [
    "dist/*"
  ]
}
